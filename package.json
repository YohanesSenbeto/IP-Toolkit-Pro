{
  "name": "ip-tool-kit-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "web:dev": "next dev --turbopack",
    "dev": "concurrently \"npm run web:dev\" \"npm run bot:dev\"",
    "web:build": "next build",
    "build": "concurrently \"npm run web:build\" \"npm run bot:build\"",
    "start": "next start",
    "dev:all": "concurrently \"npm run dev\" \"npm run bot:dev\"",
    "build:all": "concurrently \"npm run build\" \"npm run bot:build\"",
    "bot:build": "tsc -p tsconfig.bot.json",
    "bot:start": "node dist-bot/bot.js",
    "bot:dev": "concurrently \"tsc -p tsconfig.bot.json -w\" \"nodemon --watch dist-bot/bot.js --exec node dist-bot/bot.js\"",
    "bot": "npm run bot:build && npm run bot:start",
    "start:all": "concurrently \"npm run start\" \"npm run bot\"",
    "lint": "next lint"
  },
